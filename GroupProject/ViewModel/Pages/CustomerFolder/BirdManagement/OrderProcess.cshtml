@page
@model ViewModel.Pages.CustomerFolder.BirdManagement.OrderProcessModel
@{
}
<form method="post">
    <label for="address">Address:</label>
    <input type="text" id="address" name="address" />

    <button type="submit">Submit</button>
</form>
<div id="map" style="width:100%;height:500px"></div>

@section Scripts {
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDPj-amNvMEG1JK3XNvbG5khq18koYw-Nw"></script>
    <script>
        new google.maps.Map(document.getElementById("map"), {
            zoom: 15,
            center: new google.maps.LatLng(46.288896, -79.44192),
            mapTypeId: google.maps.MapTypeId.ROADMAP
        });
    </script>
}